OBJS=  basic.o arith.o bool.o symbol.o runtime.o io.o string.o list.o \
       vector.o support.o bitset.o debug.o files.o

CFLAGS=-g -I..
CC=gcc

all: librun.a

clean:
	rm -f *.o librun.a

librun.a: $(OBJS)
	-rm librun.a
	ar r librun.a $(OBJS)
	ranlib librun.a

depend:
	makedepend -f .depend $(CFLAGS) *.c 2>/dev/null

include .depend
