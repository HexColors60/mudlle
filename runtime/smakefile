OBJS=  basic.o arith.o io.o string.o runtime.o list.o bool.o vector.o symbol.o \
       support.o bitset.o debug.o files.o

DEBUG=
CFLAGS=$(DEBUG)

all: run.lib

clean:
	rm -f *.o run.lib

run.lib: $(OBJS)
	execute < <
rm -f t:*.o t:run.lib
copy $(OBJS) t: quiet
cd t:
oml t:run.lib r $(OBJS)
rm -f *.o
<
	copy t:run.lib ""

basic.o: basic.c
arith.o: arith.c
io.o: io.c
string.o: string.c
runtime.o: runtime.c
list.o: list.c
bool.o: bool.c
vector.o: vector.c
symbol.o: symbol.c
support.o: support.c
bitste.o: bitset.c
